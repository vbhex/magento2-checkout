<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="marketplace_styles"/>
    <update handle="customer_account"/>
    <head>
        <css src="Webkul_Marketplace::colorpicker/css/colorpicker.css"/>
    </head>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">VbhexCheckout Setting Account</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Vbhex\Checkout\Block\Account\Settings" name="vc_account_settings" template="account/settings.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>
